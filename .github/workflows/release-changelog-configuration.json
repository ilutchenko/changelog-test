{
    "categories": [
      {
        "title": "## ðŸš€ Features",
        "labels": ["feature"]
      },
      {
        "title": "## ðŸ›  Fixes",
        "labels": ["fix"]
      }
    ],
    "ignore_labels": [
      "ignore"
    ],
    "sort": "ASC",
    "pr_template": "- ${{TITLE}} [#${{NUMBER}}]",
    "empty_template": "- no changes",
    "label_extractor": [
      {
        "pattern": "(.) (.+)",
        "target": "$1",
        "flags": "gu"
      },
      {
        "pattern": "\\[Issue\\]",
        "on_property": "title",
        "method": "match"
      }
    ],
    "duplicate_filter": {
      "pattern": "\\[ABC-....\\]",
      "on_property": "title",
      "method": "match"
    },
    "max_tags_to_fetch": 200,
    "max_pull_requests": 200,
    "max_back_track_time_days": 365,
    "base_branches": [
      "master"
    ]
}
